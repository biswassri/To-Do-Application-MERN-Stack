{
  "name": "todoapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./dist/bin/www.js",
    "transpile": "babel ./server --out-dir dist",
    "build": "npm-run-all clean transpile",
    "watch:dev": "nodemon",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ],
    "ignore": []
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "mongoose": "^5.10.15",
    "morgan": "~1.9.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
